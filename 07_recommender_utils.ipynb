    "def batch_sentiment_analysis(reviews: List[str], batch_size: int = 32) -> List[Tuple[str, float]]:\n",
    "    \"\"\"\n",
    "    Process multiple reviews in batches for efficient sentiment analysis\n",
    "    \n",
    "    Args:\n",
    "        reviews: List of review texts\n",
    "        batch_size: Number of reviews to process at once\n",
    "    \n",
    "    Returns:\n",
    "        List of (sentiment, confidence) tuples\n",
    "    \"\"\"\n",
    "    results = []\n",
    "    \n",
    "    print(f\"Processing {len(reviews)} reviews in batches of {batch_size}...\")\n",
    "    \n",
    "    for i in range(0, len(reviews), batch_size):\n",
    "        batch = reviews[i:i + batch_size]\n",
    "        batch_results = []\n",
    "        \n",
    "        for review in batch:\n",
    "            sentiment, confidence = recommender.predict_user_sentiment(review)\n",
    "            batch_results.append((sentiment, confidence))\n",
    "        \n",
    "        results.extend(batch_results)\n",
    "        \n",
    "        if (i // batch_size + 1) % 10 == 0:\n",
    "            print(f\"Processed {i + len(batch)}/{len(reviews)} reviews\")\n",
    "    \n",
    "    return results\n",
    "\n",
    "def batch_recommendations(reviews: List[str], locations: List[str] = None, \n",
    "                         categories: List[str] = None, top_k: int = 5) -> List[List[Dict]]:\n",
    "    \"\"\"\n",
    "    Generate recommendations for multiple user reviews\n",
    "    \n",
    "    Args:\n",
    "        reviews: List of user review texts\n",
    "        locations: List of preferred locations (optional)\n",
    "        categories: List of preferred categories (optional)\n",
    "        top_k: Number of recommendations per review\n",
    "    \n",
    "    Returns:\n",
    "        List of recommendation lists\n",
    "    \"\"\"\n",
    "    if locations is None:\n",
    "        locations = [None] * len(reviews)\n",
    "    if categories is None:\n",
    "        categories = [None] * len(reviews)\n",
    "    \n",
    "    all_recommendations = []\n",
    "    \n",
    "    for i, (review, location, category) in enumerate(zip(reviews, locations, categories)):\n",
    "        print(f\"\\nProcessing recommendation {i+1}/{len(reviews)}...\")\n",
    "        \n",
    "        # Use the enhanced recommendation function but suppress output\n",
    "        import sys\n",
    "        from io import StringIO\n",
    "        \n",
    "        old_stdout = sys.stdout\n",
    "        sys.stdout = StringIO()\n",
    "        \n",
    "        try:\n",
    "            recommendations = recommend_businesses_from_review(\n",
    "                user_review=review,\n",
    "                user_location=location,\n",
    "                category_preference=category,\n",
    "                top_k=top_k\n",
    "            )\n",
    "        finally:\n",
    "            sys.stdout = old_stdout\n",
    "        \n",
    "        all_recommendations.append(recommendations)\n",
    "    \n",
    "    return all_recommendations"
   ]
  },
