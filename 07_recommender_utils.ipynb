    "## System Health Check"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def system_health_check() -> Dict:\n",
    "    \"\"\"\n",
    "    Perform comprehensive system health check\n",
    "    \n",
    "    Returns:\n",
    "        Health status dictionary\n",
    "    \"\"\"\n",
    "    print(\"Running system health check...\")\n",
    "    \n",
    "    health_status = {\n",
    "        'overall_status': 'HEALTHY',\n",
    "        'checks': {},\n",
    "        'timestamp': datetime.now().isoformat()\n",
    "    }\n",
    "    \n",
    "    # Check 1: Model loading\n",
    "    try:\n",
    "        test_input = \"This is a test review.\"\n",
    "        sentiment, confidence = recommender.predict_user_sentiment(test_input)\n",
    "        health_status['checks']['model_loading'] = {\n",
    "            'status': 'PASS',\n",
    "            'message': f'Model working, predicted: {sentiment} (confidence: {confidence:.3f})'\n",
    "        }\n",
    "    except Exception as e:\n",
    "        health_status['checks']['model_loading'] = {\n",
    "            'status': 'FAIL',\n",
    "            'message': f'Model error: {str(e)}'\n",
    "        }\n",
    "        health_status['overall_status'] = 'UNHEALTHY'\n",
    "    \n",
    "    # Check 2: Data integrity\n",
    "    try:\n",
    "        assert len(business_df) > 0, \"Business data is empty\"\n",
    "        assert len(merged_df) > 0, \"Review data is empty\"\n",
    "        assert len(sentiment_profile_df) > 0, \"Sentiment profiles are empty\"\n",
    "        \n",
    "        health_status['checks']['data_integrity'] = {\n",
    "            'status': 'PASS',\n",
    "            'message': f'Data loaded: {len(business_df)} businesses, {len(merged_df)} reviews'\n",
    "        }\n",
    "    except Exception as e:\n",
    "        health_status['checks']['data_integrity'] = {\n",
    "            'status': 'FAIL',\n",
    "            'message': f'Data error: {str(e)}'\n",
    "        }\n",
    "        health_status['overall_status'] = 'UNHEALTHY'\n",
    "    \n",
    "    # Check 3: Recommendation generation\n",
    "    try:\n",
    "        test_recommendations = recommender.find_similar_businesses(\n",
    "            business_df['business_id'].iloc[0], top_k=3\n",
    "        )\n",
    "        assert len(test_recommendations) > 0, \"No recommendations generated\"\n",
    "        \n",
    "        health_status['checks']['recommendation_generation'] = {\n",
    "            'status': 'PASS',\n",
    "            'message': f'Generated {len(test_recommendations)} recommendations'\n",
    "        }\n",
    "    except Exception as e:\n",
    "        health_status['checks']['recommendation_generation'] = {\n",
    "            'status': 'FAIL',\n",
    "            'message': f'Recommendation error: {str(e)}'\n",
    "        }\n",
    "        health_status['overall_status'] = 'UNHEALTHY'\n",
    "    \n",
    "    # Check 4: Feature matrix\n",
    "    try:\n",
    "        assert hasattr(recommender, 'feature_matrix'), \"Feature matrix not found\"\n",
    "        assert recommender.feature_matrix.shape[0] > 0, \"Feature matrix is empty\"\n",
    "        \n",
    "        health_status['checks']['feature_matrix'] = {\n",
    "            'status': 'PASS',\n",
    "            'message': f'Feature matrix shape: {recommender.feature_matrix.shape}'\n",
    "        }\n",
    "    except Exception as e:\n",
    "        health_status['checks']['feature_matrix'] = {\n",
    "            'status': 'FAIL',\n",
    "            'message': f'Feature matrix error: {str(e)}'\n",
    "        }\n",
    "        health_status['overall_status'] = 'UNHEALTHY'\n",
    "    \n",
    "    # Print results\n",
    "    print(\"\\n\" + \"=\" * 50)\n",
    "    print(\"SYSTEM HEALTH CHECK RESULTS\")\n",
    "    print(\"=\" * 50)\n",
    "    \n",
    "    status_emoji = \"✅\" if health_status['overall_status'] == 'HEALTHY' else \"❌\"\n",
    "    print(f\"\\n{status_emoji} Overall Status: {health_status['overall_status']}\")\n",
    "    \n",
    "    for check_name, check_result in health_status['checks'].items():\n",
    "        emoji = \"✅\" if check_result['status'] == 'PASS' else \"❌\"\n",
    "        print(f\"\\n{emoji} {check_name.replace('_', ' ').title()}:\")\n",
    "        print(f\"   Status: {check_result['status']}\")\n",
    "        print(f\"   Message: {check_result['message']}\")\n",
    "    \n",
    "    return health_status\n",
    "\n",
    "# Run health check\n",
    "health_status = system_health_check()"
   ]
