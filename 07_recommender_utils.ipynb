    "## Performance Monitoring"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def benchmark_system_performance(num_tests: int = 100) -> Dict:\n",
    "    \"\"\"\n",
    "    Benchmark the recommendation system performance\n",
    "    \n",
    "    Args:\n",
    "        num_tests: Number of test cases to run\n",
    "    \n",
    "    Returns:\n",
    "        Performance metrics dictionary\n",
    "    \"\"\"\n",
    "    print(f\"Running performance benchmark with {num_tests} test cases...\")\n",
    "    \n",
    "    # Sample test reviews\n",
    "    test_reviews = [\n",
    "        \"Great food and excellent service!\",\n",
    "        \"Terrible experience, would not recommend.\",\n",
    "        \"Average place, nothing special.\",\n",
    "        \"Outstanding quality and friendly staff.\",\n",
    "        \"Poor service and overpriced food.\"\n",
    "    ] * (num_tests // 5 + 1)\n",
    "    \n",
    "    test_reviews = test_reviews[:num_tests]\n",
    "    \n",
    "    # Benchmark sentiment analysis\n",
    "    start_time = time.time()\n",
    "    sentiment_results = batch_sentiment_analysis(test_reviews, batch_size=16)\n",
    "    sentiment_time = time.time() - start_time\n",
    "    \n",
    "    # Benchmark recommendations (smaller sample)\n",
    "    sample_size = min(10, num_tests)\n",
    "    start_time = time.time()\n",
    "    recommendation_results = batch_recommendations(test_reviews[:sample_size], top_k=3)\n",
    "    recommendation_time = time.time() - start_time\n",
    "    \n",
    "    # Calculate metrics\n",
    "    metrics = {\n",
    "        'sentiment_analysis': {\n",
    "            'total_time': sentiment_time,\n",
    "            'avg_time_per_review': sentiment_time / num_tests,\n",
    "            'reviews_per_second': num_tests / sentiment_time,\n",
    "            'total_reviews': num_tests\n",
    "        },\n",
    "        'recommendations': {\n",
    "            'total_time': recommendation_time,\n",
    "            'avg_time_per_request': recommendation_time / sample_size,\n",
    "            'requests_per_second': sample_size / recommendation_time,\n",
    "            'total_requests': sample_size\n",
    "        },\n",
    "        'system_info': {\n",
    "            'device': str(device),\n",
    "            'timestamp': datetime.now().isoformat(),\n",
    "            'business_count': len(business_df),\n",
    "            'review_count': len(merged_df)\n",
    "        }\n",
    "    }\n",
    "    \n",
    "    # Print results\n",
    "    print(\"\\n\" + \"=\" * 50)\n",
    "    print(\"PERFORMANCE BENCHMARK RESULTS\")\n",
    "    print(\"=\" * 50)\n",
    "    \n",
    "    print(f\"\\n📊 Sentiment Analysis:\")\n",
    "    print(f\"   • Total Time: {sentiment_time:.2f} seconds\")\n",
    "    print(f\"   • Avg Time per Review: {metrics['sentiment_analysis']['avg_time_per_review']:.3f} seconds\")\n",
    "    print(f\"   • Reviews per Second: {metrics['sentiment_analysis']['reviews_per_second']:.1f}\")\n",
    "    \n",
    "    print(f\"\\n🎯 Recommendations:\")\n",
    "    print(f\"   • Total Time: {recommendation_time:.2f} seconds\")\n",
    "    print(f\"   • Avg Time per Request: {metrics['recommendations']['avg_time_per_request']:.3f} seconds\")\n",
    "    print(f\"   • Requests per Second: {metrics['recommendations']['requests_per_second']:.1f}\")\n",
    "    \n",
    "    print(f\"\\n💻 System Info:\")\n",
    "    print(f\"   • Device: {device}\")\n",
    "    print(f\"   • Business Count: {len(business_df):,}\")\n",
    "    print(f\"   • Review Count: {len(merged_df):,}\")\n",
    "    \n",
    "    return metrics\n",
    "\n",
    "# Run benchmark\n",
    "performance_metrics = benchmark_system_performance(50)"
   ]
